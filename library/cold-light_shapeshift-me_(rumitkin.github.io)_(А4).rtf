\u1042\'c2\u1076\'e4\u1088\'f0\u1091\'f3\u1075\'e3 \u1085\'ed\u1072\'e0\u1089\'f1 \u1086\'ee\u1073\'e1\u1086\'ee\u1075\'e3\u1085\'ed\u1072\'e0\u1083\'eb \u1086\'ee\u1075\'e3\u1088\'f0\u1086\'ee\u1084\'ec\u1085\'ed\u1099\'fb\u1081\'e9 \u1089\'f1\u1077\'e5\u1088\'f0\u1099\'fb\u1081\'e9 \u1079\'e7\u1074\'e2\u1077\'e5\u1088\'f0\u1100\'fc. \u1054\'ce\u1096\'f8\u1077\'e5\u1081\'e9\u1085\'ed\u1080\'e8\u1082\'ea\u1072\'e0 \u1085\'ed\u1072\'e0 \u1085\'ed\u1077\'e5\u1084\'ec \u1085\'ed\u1077\'e5 \u1073\'e1\u1099\'fb\u1083\'eb\u1086\'ee. \u1064\'d8\u1077\'e5\u1088\'f0\u1089\'f1\u1090\'f2\u1100\'fc \u1085\'ed\u1072\'e0 \u1089\'f1\u1087\'ef\u1080\'e8\u1085\'ed\u1077\'e5 \u1086\'ee\u1090\'f2\u1083\'eb\u1080\'e8\u1074\'e2\u1072\'e0\u1083\'eb\u1072\'e0 \u1082\'ea\u1086\'ee\u1088\'f0\u1080\'e8\u1095\'f7\u1085\'ed\u1077\'e5\u1074\'e2\u1099\'fb\u1084\'ec, \u1085\'ed\u1072\'e0 \u1078\'e6\u1080\'e8\u1074\'e2\u1086\'ee\u1090\'f2\u1077\'e5 \u1089\'f1\u1074\'e2\u1077\'e5\u1090\'f2\u1083\'eb\u1077\'e5\u1083\'eb\u1072\'e0 \u1080\'e8 \u1082\'ea\u1072\'e0\u1079\'e7\u1072\'e0\u1083\'eb\u1072\'e0\u1089\'f1\u1100\'fc \u1073\'e1\u1077\'e5\u1083\'eb\u1086\'ee\u1081\'e9. \u1057\'d1\u1086\'ee\u1073\'e1\u1072\'e0\u1082\'ea\u1072\'e0 \u8211\'96 \u1090\'f2\u1086\'ee\u1095\'f7\u1085\'ed\u1077\'e5\u1077\'e5, \u1103\'ff \u1087\'ef\u1086\'ee\u1076\'e4\u1091\'f3\u1084\'ec\u1072\'e0\u1083\'eb\u1072\'e0, \u1095\'f7\u1090\'f2\u1086\'ee \u1101\'fd\u1090\'f2\u1086\'ee \u1089\'f1\u1086\'ee\u1073\'e1\u1072\'e0\u1082\'ea\u1072\'e0 \u8211\'96 \u1087\'ef\u1088\'f0\u1099\'fb\u1075\'e3\u1085\'ed\u1091\'f3\u1083\'eb\u1072\'e0 \u1074\'e2 \u1075\'e3\u1083\'eb\u1091\'f3\u1073\'e1\u1086\'ee\u1082\'ea\u1080\'e8\u1081\'e9 \u1089\'f1\u1085\'ed\u1077\'e5\u1075\'e3, \u1090\'f2\u1080\'e8\u1093\'f5\u1086\'ee \u1088\'f0\u1099\'fb\u1095\'f7\u1072\'e0, \u1074\'e2\u1088\'f0\u1099\'fb\u1083\'eb\u1072\'e0\u1089\'f1\u1100\'fc \u1074\'e2 \u1085\'ed\u1077\'e5\u1075\'e3\u1086\'ee, \u1087\'ef\u1086\'ee\u1090\'f2\u1086\'ee\u1084\'ec \u1087\'ef\u1086\'ee\u1076\'e4\u1085\'ed\u1103\'ff\u1083\'eb\u1072\'e0 \u1075\'e3\u1086\'ee\u1083\'eb\u1086\'ee\u1074\'e2\u1091\'f3 \u1080\'e8 \u1087\'ef\u1086\'ee\u1089\'f1\u1084\'ec\u1086\'ee\u1090\'f2\u1088\'f0\u1077\'e5\u1083\'eb\u1072\'e0 \u1085\'ed\u1072\'e0 \u1085\'ed\u1072\'e0\u1089\'f1. \u1043\'c3\u1083\'eb\u1072\'e0\u1079\'e7\u1072\'e0 \u1091\'f3 \u1085\'ed\u1077\'e5\u1077\'e5 \u1073\'e1\u1099\'fb\u1083\'eb\u1080\'e8 \u1078\'e6\u1077\'e5\u1083\'eb\u1090\'f2\u1099\'fb\u1077\'e5, \u1084\'ec\u1086\'ee\u1088\'f0\u1076\'e4\u1072\'e0 \u1074\'e2\u1099\'fb\u1090\'f2\u1103\'ff\u1085\'ed\u1091\'f3\u1090\'f2\u1072\'e0\u1103\'ff. \u1048\'c8\u1079\'e7\u1086\'ee \u1088\'f0\u1090\'f2\u1072\'e0 \u1074\'e2\u1099\'fb\u1088\'f0\u1099\'fb\u1074\'e2\u1072\'e0\u1083\'eb\u1089\'f1\u1103\'ff \u1087\'ef\u1072\'e0\u1088\'f0. \u1055\'cf\u1088\'f0\u1086\'ee\u1096\'f8\u1083\'eb\u1072\'e0 \u1089\'f1\u1077\'e5\u1082\'ea\u1091\'f3\u1085\'ed\u1076\'e4\u1072\'e0, \u1080\'e8 \u1086\'ee\u1085\'ed\u1072\'e0 \u1087\'ef\u1088\'f0\u1099\'fb\u1075\'e3\u1085\'ed\u1091\'f3\u1083\'eb\u1072\'e0 \u1086\'ee\u1073\'e1\u1088\'f0\u1072\'e0\u1090\'f2\u1085\'ed\u1086\'ee \u1085\'ed\u1072\'e0 \u1076\'e4\u1086\'ee\u1088\'f0\u1086\'ee\u1075\'e3\u1091\'f3, \u1072\'e0 \u1087\'ef\u1086\'ee\u1090\'f2\u1086\'ee\u1084\'ec \u1087\'ef\u1086\'ee\u1096\'f8\u1083\'eb\u1072\'e0 \u1087\'ef\u1088\'f0\u1103\'ff\u1084\'ec\u1086\'ee \u1085\'ed\u1072\'e0 \u1057\'d1\u1072\'e0\u1085\'ed\u1102\'fe. \u1041\'c1\u1088\'f0\u1072\'e0\u1090\'f2 \u1085\'ed\u1077\'e5 \u1080\'e8\u1089\'f1\u1087\'ef\u1091\'f3\u1075\'e3\u1072\'e0\u1083\'eb\u1089\'f1\u1103\'ff, \u1090\'f2\u1086\'ee\u1083\'eb\u1100\'fc\u1082\'ea\u1086\'ee \u1091\'f3\u1076\'e4\u1080\'e8\u1074\'e2\u1083\'eb\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee \u1086\'ee\u1090\'f2\u1089\'f1\u1090\'f2\u1091\'f3\u1087\'ef\u1080\'e8\u1083\'eb \u1085\'ed\u1072\'e0\u1079\'e7\u1072\'e0\u1076\'e4. }
\par \pard\plain \s0\sl276\slmult1\qj\widctlpar\hyphpar1\faauto\li0\ri0\lin0\rin0\fi709\ltrpar\aspalpha\dbch\af15\langfe1049\dbch\af19\afs24\alang1025\cf0\kerning1\loch\f4\fs28\lang1049\qj\li0\ri0\lin0\rin0\fi567{\dbch\af17\afs28\rtlch \ltrch\fs28\loch\f4\hich\af4
\u8211\'96 }{\dbch\af17\afs28\rtlch \ltrch\loch\fs28\loch\f4\hich\af4
\u1056\'d0\u1072\'e0\u1081\'e9\u1083\'eb\u1080\'e8, \u8211\'96 \u1085\'ed\u1077\'e5\u1075\'e3\u1088\'f0\u1086\'ee\u1084\'ec\u1082\'ea\u1080\'e8\u1081\'e9 \u1086\'ee\u1082\'ea\u1088\'f0\u1080\'e8\u1082\'ea \u1089\'f1\u1079\'e7\u1072\'e0\u1076\'e4\u1080\'e8. }
\par \pard\plain \s0\sl276\slmult1\qj\widctlpar\hyphpar1\faauto\li0\ri0\lin0\rin0\fi709\ltrpar\aspalpha\dbch\af15\langfe1049\dbch\af19\afs24\alang1025\cf0\kerning1\loch\f4\fs28\lang1049\qj\li0\ri0\lin0\rin0\fi567{\dbch\af17\afs28\rtlch \ltrch\loch\fs28\loch\f4\hich\af4
\u1047\'c7\u1074\'e2\u1077\'e5\u1088\'f0\u1100\'fc, \u1091\'f3\u1078\'e6\u1077\'e5 \u1074\'e2\u1089\'f1\u1090\'f2\u1072\'e0\u1074\'e2\u1096\'f8\u1080\'e8\u1081\'e9 \u1085\'ed\u1072\'e0 \u1079\'e7\u1072\'e0\u1076\'e4\u1085\'ed\u1080\'e8\u1077\'e5 \u1083\'eb\u1072\'e0\u1087\'ef\u1099\'fb, \u1080\'e8\u1079\'e7\u1084\'ec\u1077\'e5\u1085\'ed\u1080\'e8\u1083\'eb \u1090\'f2\u1088\'f0\u1072\'e0\u1077\'e5\u1082\'ea\u1090\'f2\u1086\'ee\u1088\'f0\u1080\'e8\u1102\'fe \u1076\'e4\u1074\'e2\u1080\'e8\u1078\'e6\u1077\'e5\u1085\'ed\u1080\'e8\u1103\'ff, \u1091\'f3\u1087\'ef\u1072\'e0\u1083\'eb \u1085\'ed\u1072\'e0 \u1073\'e1\u1086\'ee\u1082\'ea, \u1087\'ef\u1077\'e5\u1088\'f0\u1077\'e5\u1082\'ea\u1072\'e0\u1090\'f2\u1080\'e8\u1083\'eb\u1089\'f1\u1103\'ff \u1095\'f7\u1077\'e5\u1088\'f0\u1077\'e5\u1079\'e7 \u1089\'f1\u1087\'ef\u1080\'e8\u1085\'ed\u1091\'f3 \u1080\'e8, \u1086\'ee\u1090\'f2\u1088\'f0\u1103\'ff\u1093\'f5\u1080\'e8\u1074\'e2\u1072\'e0\u1103\'ff\u1089\'f1\u1100\'fc \u1086\'ee\u1090\'f2 \u1089\'f1\u1085\'ed\u1077\'e5\u1075\'e3\u1072\'e0, \u1087\'ef\u1086\'ee\u1090\'f2\u1088\'f0\u1091\'f3\u1089\'f1\u1080\'e8\u1083\'eb \u1082\'ea \u1093\'f5\u1086\'ee\u1079\'e7\u1103\'ff\u1080\'e8\u1085\'ed\u1091\'f3. \u1052\'cc\u1099\'fb \u1086\'ee\u1075\'e3\u1083\'eb\u1103\'ff\u1085\'ed\u1091\'f3\u1083\'eb\u1080\'e8\u1089\'f1\u1100\'fc. \u1052\'cc\u1077\'e5\u1090\'f2\u1088\'f0\u1072\'e0\u1093\'f5 \u1074\'e2 \u1076\'e4\u1074\'e2\u1072\'e0\u1076\'e4\u1094\'f6\u1072\'e0\u1090\'f2\u1080\'e8 \u1087\'ef\u1086\'ee\u1079\'e7\u1072\'e0\u1076\'e4\u1080\'e8 \u1085\'ed\u1072\'e0\u1089\'f1 \u1096\'f8\u1077\'e5\u1083\'eb \u1084\'ec\u1091\'f3\u1078\'e6\u1095\'f7\u1080\'e8\u1085\'ed\u1072\'e0 \u1074\'e2 \u1085\'ed\u1077\'e5\u1087\'ef\u1088\'f0\u1080\'e8\u1084\'ec\u1077\'e5\u1090\'f2\u1085\'ed\u1086\'ee\u1081\'e9 \u1089\'f1\u1077\'e5\u1088\'f0\u1086\'ee-\u1079\'e7\u1077\'e5\u1083\'eb\u1077\'e5\u1085\'ed\u1086\'ee\u1081\'e9 \u1082\'ea\u1091\'f3\u1088\'f0\u1090\'f2\u1082\'ea\u1077\'e5. \u1064\'d8\u1072\'e0\u1087\'ef\u1082\'ea\u1091\'f3 \u1086\'ee\u1085\'ed \u1085\'ed\u1077\'e5 \u1085\'ed\u1086\'ee\u1089\'f1\u1080\'e8\u1083\'eb. \u1042\'c2\u1086\'ee\u1083\'eb\u1086\'ee\u1089\'f1\u1099\'fb \u1073\'e1\u1099\'fb\u1083\'eb\u1080\'e8 \u1075\'e3\u1091\'f3\u1089\'f1\u1090\'f2\u1099\'fb\u1077\'e5 \u1080\'e8 \u1089\'f1\u1074\'e2\u1077\'e5\u1090\'f2\u1083\'eb\u1099\'fb\u1077\'e5, \u1087\'ef\u1086\'ee \u1094\'f6\u1074\'e2\u1077\'e5\u1090\'f2\u1091\'f3 \u1090\'f2\u1072\'e0\u1082\'ea\u1080\'e8\u1077\'e5 \u1078\'e6\u1077\'e5, \u1082\'ea\u1072\'e0\u1082\'ea \u1091\'f3 \u1084\'ec\u1077\'e5\u1085\'ed\u1103\'ff \u1080\'e8 \u1073\'e1\u1088\'f0\u1072\'e0\u1090\'f2\u1072\'e0. \u1045\'c5\u1075\'e3\u1086\'ee \u1079\'e7\u1074\'e2\u1072\'e0\u1083\'eb\u1080\'e8 \u1042\'c2\u1072\'e0\u1083\'eb\u1090\'f2\u1090\'f2\u1077\'e5\u1088\'f0\u1080\'e8 \u1051\'cb\u1072\'e0\u1081\'e9\u1085\'ed\u1077\'e5, \u1085\'ed\u1086\'ee \u1103\'ff \u1085\'ed\u1077\'e5 \u1079\'e7\u1085\'ed\u1072\'e0\u1083\'eb\u1072\'e0 \u1086\'ee\u1073\'e1 \u1101\'fd\u1090\'f2\u1086\'ee\u1084\'ec. \u1044\'c4\u1083\'eb\u1103\'ff \u1084\'ec\u1077\'e5\u1085\'ed\u1103\'ff \u1086\'ee\u1085\'ed \u1073\'e1\u1099\'fb\u1083\'eb \u1074\'e2\u1089\'f1\u1077\'e5\u1075\'e3\u1086\'ee \u1083\'eb\u1080\'e8\u1096\'f8\u1100\'fc \u1085\'ed\u1077\'e5\u1079\'e7\u1085\'ed\u1072\'e0\u1082\'ea\u1086\'ee\u1084\'ec\u1077\'e5\u1094\'f6 \u1089\'f1 \u1089\'f1\u1086\'ee\u1073\'e1\u1072\'e0\u1082\'ea\u1086\'ee\u1081\'e9. }
\par \pard\plain \s0\sl276\slmult1\qj\widctlpar\hyphpar1\faauto\li0\ri0\lin0\rin0\fi709\ltrpar\aspalpha\dbch\af15\langfe1049\dbch\af19\afs24\alang1025\cf0\kerning1\loch\f4\fs28\lang1049\qj\li0\ri0\lin0\rin0\fi567{\dbch\af17\afs28\rtlch \ltrch\fs28\loch\f4\hich\af4
\u8211\'96 }{\dbch\af17\afs28\rtlch \ltrch\loch\fs28\loch\f4\hich\af4
