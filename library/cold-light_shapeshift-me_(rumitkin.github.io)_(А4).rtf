\par \pard\plain \s0\sl276\slmult1\qj\widctlpar\hyphpar1\faauto\li0\ri0\lin0\rin0\fi709\ltrpar\aspalpha\dbch\af15\langfe1049\dbch\af19\afs24\alang1025\cf0\kerning1\loch\f4\fs28\lang1049\qj\li0\ri0\lin0\rin0\fi567{\dbch\af17\afs28\rtlch \ltrch\loch\fs28\loch\f4\hich\af4
\u1054\'ce\u1085\'ed \u1087\'ef\u1086\'ee\u1074\'e2\u1077\'e5\u1088\'f0\u1085\'ed\u1091\'f3\u1083\'eb \u1075\'e3\u1086\'ee\u1083\'eb\u1086\'ee\u1074\'e2\u1091\'f3 \u1074\'e2 \u1084\'ec\u1086\'ee\u1102\'fe \u1089\'f1\u1090\'f2\u1086\'ee\u1088\'f0\u1086\'ee\u1085\'ed\u1091\'f3. \u1071\'df \u1079\'e7\u1085\'ed\u1072\'e0\u1083\'eb\u1072\'e0, \u1095\'f7\u1090\'f2\u1086\'ee \u1086\'ee\u1085\'ed \u1085\'ed\u1077\'e5 \u1074\'e2\u1080\'e8\u1076\'e4\u1080\'e8\u1090\'f2 \u1084\'ec\u1077\'e5\u1085\'ed\u1103\'ff, \u1086\'ee\u1088\'f0\u1080\'e8\u1077\'e5\u1085\'ed\u1090\'f2\u1080\'e8\u1088\'f0\u1091\'f3\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff \u1090\'f2\u1086\'ee\u1083\'eb\u1100\'fc\u1082\'ea\u1086\'ee \u1085\'ed\u1072\'e0 \u1075\'e3\u1086\'ee\u1083\'eb\u1086\'ee\u1089\'f1. }
\par \pard\plain \s0\sl276\slmult1\qj\widctlpar\hyphpar1\faauto\li0\ri0\lin0\rin0\fi709\ltrpar\aspalpha\dbch\af15\langfe1049\dbch\af19\afs24\alang1025\cf0\kerning1\loch\f4\fs28\lang1049\qj\li0\ri0\lin0\rin0\fi567{\dbch\af17\afs28\rtlch \ltrch\fs28\loch\f4\hich\af4
\u8211\'96 }{\dbch\af17\afs28\rtlch \ltrch\loch\fs28\loch\f4\hich\af4
\u1051\'cb\u1080\'e8\u1079\'e7\u1072\'e0, \u1087\'ef\u1086\'ee\u1078\'e6\u1072\'e0\u1083\'eb\u1091\'f3\u1081\'e9\u1089\'f1\u1090\'f2\u1072\'e0, \u1080\'e8\u1076\'e4\u1080\'e8 \u1076\'e4\u1086\'ee\u1084\'ec\u1086\'ee\u1081\'e9, \u8211\'96 \u1087\'ef\u1086\'ee\u1087\'ef\u1088\'f0\u1086\'ee\u1089\'f1\u1080\'e8\u1083\'eb \u1086\'ee\u1085\'ed. \u8211\'96 \u1063\'d7\u1077\'e5\u1088\'f0\u1077\'e5\u1079\'e7 \u1076\'e4\u1074\'e2\u1072\'e0\u1076\'e4\u1094\'f6\u1072\'e0\u1090\'f2\u1100\'fc \u1083\'eb\u1077\'e5\u1090\'f2 \u1090\'f2\u1099\'fb \u1079\'e7\u1072\'e0 \u1085\'ed\u1072\'e0\u1089\'f1 \u1076\'e4\u1074\'e2\u1086\'ee\u1080\'e8\u1093\'f5 \u1087\'ef\u1086\'ee\u1089\'f1\u1090\'f2\u1088\'f0\u1086\'ee\u1080\'e8\u1096\'f8\u1100\'fc \u1085\'ed\u1072\'e0 \u1076\'e4\u1077\'e5\u1088\'f0\u1077\'e5\u1074\'e2\u1077\'e5 \u1076\'e4\u1086\'ee\u1084\'ec\u1080\'e8\u1082\'ea \u1076\'e4\u1083\'eb\u1103\'ff \u1089\'f1\u1074\'e2\u1086\'ee\u1080\'e8\u1093\'f5 \u1076\'e4\u1077\'e5\u1090\'f2\u1077\'e5\u1081\'e9.}
